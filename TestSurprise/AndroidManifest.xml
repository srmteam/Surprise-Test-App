<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.testsurprise"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-permission android:name="android.permission.INTERNET" />

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="17" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.testsurprise.TestMainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.testsurprise.Login"
            android:label="@string/title_activity_login"
            android:parentActivityName="com.example.testsurprise" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.testsurprise" />
        </activity>
        <activity
            android:name="com.example.testsurprise.TestCode"
            android:label="@string/title_activity_test_code"
            android:parentActivityName="com.example.testsurprise" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.testsurprise" />
        </activity>
        <activity
            android:name="com.example.testsurprise.Result"
            android:label="@string/title_activity_result"
            android:parentActivityName="com.example.testsurprise" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.testsurprise" />
        </activity>
        <activity
            android:name="com.example.testsurprise.Test"
            android:label="@string/title_activity_test"
            android:parentActivityName="com.example.testsurprise" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.testsurprise" />
        </activity>
        <activity
            android:name="com.example.testsurprise.ResultFinal"
            android:label="@string/title_activity_result_final"
            android:parentActivityName="com.example.testsurprise" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.testsurprise" />
        </activity>
        <activity
            android:name="com.example.testsurprise.EvarsityFinal"
            android:label="@string/title_activity_evarsity_final" >
        </activity>
    </application>

</manifest>
